{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@nrwl/cli/bin/nx.js",
      "cwd": "${fileDirname}",
      "sourceMaps": true,
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development",
        "NX_WORKSPACE_ROOT_PATH": "${fileDirname}"
      }
    }
  ]
}
